{
  "name": "pac-atac",
  "repository": "Pacman3D",
  "version": "0.1.0",
  "title": "Pac-Atac",
  "description": "Pacman in 3D!",
  "keywords": [
    "Princeton",
    "graphics",
    "three.js",
    "COS 426",
    "ivy league",
    "quant boyz"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --output-public-path \"\"",
    "prebuild": "rimraf ./build && mkdirp ./build",
    "build": "npm run prebuild && webpack --mode production --output-public-path \"\"",
    "format": "prettier --write '*.js' 'src/**/*.js'",
    "deploy": "npm run prebuild && webpack --mode production && gh-pages -d build"
  },
  "dependencies": {
    "css-loader": "^3.5.3",
    "dat.gui": "^0.7.7",
    "firebase": "^7.14.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.2.1",
    "three": "^0.137.0",
    "three-obj-loader": "^1.1.3",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^4.3.0",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.11.0"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
